<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="demo.zxg.com.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AboutActivity"/>
        <activity android:name=".SaveBitmapActivity"/>
        <activity android:name=".SaveDrawableActivity"/>
        <activity android:name=".SaveJsonArrayActivity"/>
        <activity android:name=".SaveJsonObjectActivity"/>
        <activity android:name=".SaveMediaActivity"/>
        <activity android:name=".SaveStringActivity"/>
        <activity android:name=".SaveObjectActivity"/>

    </application>

</manifest>